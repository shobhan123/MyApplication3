<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/College"
    android:background="#fff">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/logo_head"
        android:adjustViewBounds="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"


        android:id="@+id/tableRow"
        android:layout_above="@+id/linearLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="85dp"
            android:id="@+id/startCampaign"
            android:src="@drawable/mob"
            android:layout_weight="0.16"


            android:layout_gravity="left|top" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="85dp"
            android:id="@+id/reportsURL"

            android:layout_weight="0.16"
            android:src="@drawable/report23"
            android:layout_gravity="right|top" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout"
        android:layout_above="@+id/linearLayout2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Start Campaign"
            android:id="@+id/textView2"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#ff0d0d0d" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="View Report"
            android:id="@+id/textView3"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#FF0D0D0D" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:id="@+id/linearLayout2"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:id="@+id/web"
            android:src="@drawable/seoicon"
            android:layout_gravity="left|top"
            android:layout_weight="0.14" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="85dp"
            android:id="@+id/mail"

            android:layout_weight="0.16"
            android:src="@drawable/competition"
            android:layout_gravity="right|top" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:id="@+id/linearLayout3"
        android:layout_below="@+id/linearLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="     SEO"
            android:id="@+id/textView4"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#FF0D0D0D" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="          Competition"
            android:id="@+id/textView5"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#FF0D0D0D" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout4"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="85dp"
            android:src="@drawable/settings"

            android:id="@+id/imageView3"
            android:layout_weight="0.16" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="85dp"
            android:id="@+id/imageView2"
            android:src="@drawable/logout"
            android:layout_weight="0.16" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Settings"
            android:id="@+id/textView6"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#FF0D0D0D" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Log Out"
            android:id="@+id/textView7"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#FF0D0D0D" />
    </LinearLayout>


</RelativeLayout>